{
  "name": "@udhaar/server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "dev": "pnpm --stream=true run \"/dev:/\"",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "wait-on dist/index.js && node --env-file=.env.dev --enable-source-maps --watch dist/index.js",
    "build": "tsc && tsc-alias && cp .env* dist/",
    "start": "node --env-file=.env.prod dist/index.js",
    "check": "eslint src && tsc --noEmit",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "hellskater",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/sensible": "^6.0.1",
    "@trpc/server": "11.0.0-rc.604",
    "@udhaar/api": "workspace:*",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.1",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.8.4",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@udhaar/typescript-config": "workspace:*",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  }
}
